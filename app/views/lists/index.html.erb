<h1>All Lists</h1>

<ul class ="card-container">
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ul>

<button class="btn"><%= link_to "Add New List", new_list_path%></button>

